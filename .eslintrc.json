{
	"env": {
		"browser": true,
		"es2021": true,
		"es6": true,
		"node": true
	},
	"extends": ["react-app", "plugin:jsx-a11y/recommended"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react", "jsx-a11y"],
	"rules": {
		"react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}],
		"semi": [2, "never"],
		"indent": ["error", "tab"],
		"no-multi-spaces": ["error", {"ignoreEOLComments": false}],
		"eol-last": ["error", "always"]
	},
	"globals": {}
}
